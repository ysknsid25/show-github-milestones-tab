{
  "manifest_version": 3,
  "name": "GitHub Milestones Tab",
  "version": "1.0",
  "description": "Add Milestones tab to GitHub repository navigation",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
